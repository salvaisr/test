				if (pip_prices!='NaN'){
				document.addme.pip_prices.value = pip_prices}
				if (pip_ids!='undefined'){
				document.addme.pip_ids.value = pip_ids}

        }
         catch(e){}
      
         
  
        try{
        //   _gaq.push(['_link', 'https://pisgashop.e-shop5.co.il/checkout.asp']);
        }
