			                     {
			                         
			                         
			                         if (  return_cartlogic_url == "1" )
			                         {
			                             return cartlogic_url;
			                         }
			                         else
			                         {
			                             //addme_cart_ajax(pid+bundlepid+relativepid,pip_str,pip_prices,pip_ids,quan+bundlequan+relativequan,numOfItems,ItemPromotionIdentStr,BuildComputerItemsInput,CartParamsStr);

			                          
			                             $(".product-prices .order-button").hide();
