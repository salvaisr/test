	      $("body").on("click",".packagelist .quantity .quan_btn",function()
	        { 
                var $input  = $(this).parent().find(".value");
                var $action = $(this).attr("action");
                var num = parseInt ( $input.val() );
                if ( $action == "odd" ) 
                {
                  
                   $input.val(  ( (num) + 1 ) * 1 ) ;
                }
                else
                {
                    if ( num > 0 ){
                    $input.val(  ( (num) - 1 )  * 1 ) ;
                    }
                }
                calculate_package_items();
                return false;
            });
          



	        $("body").on("click",".Product_Sons_list .prs_buttons a",function()
	        { 
	            var obj   = this ;
	            var quan_multiply = 1;
	            var quan_base = 1;
	            var quan_input = $(this).parent().find(".quantityInput") ;
	            quan_multiply = $(quan_input).attr('quan_multiply');
	            quan_base     = $(quan_input).attr('quan_base');
	             
                

	                if ( $(obj).hasClass("minus") )
	                {
	                    if ( $(quan_input).html() > 0)
	                    {
	                        $(quan_input).html(( $(quan_input).html()-quan_multiply*1)*1).formatCurrency({
	                            groupDigits: true , symbol: '',
	                            roundToDecimalPlace: 3
	                        });
	                    }

	                    if ( $(quan_input).html() < quan_base)
	                    {
	                        $(quan_input).html(quan_base) ;
	                    }

	                }
	                else
	                {
	                    $(quan_input).html(( $(quan_input).html() *1) + (quan_multiply * 1)).formatCurrency({
	                        groupDigits: true , symbol: '',
	                        roundToDecimalPlace: 3
	                    });
	                }
	            

	                var quantity = $(quan_input).html() 
	                $("#_quantity_" +  $(quan_input).attr('son_id') ).val(quantity);
	            // update price
	            var Num_After_dot = "2";
	            var temp_dropDecimals = 0;
	            if (Num_After_dot == "1" || Num_After_dot == "2" )
	            {
	                temp_dropDecimals = 2;
	            }

	            if(!$.isNumeric(quan_base)){quan_base = 1;}
	            if(!$.isNumeric(quantity)){quantity = 1;}

	            var newtotal = $(this).parents(".son_wrap").find(".unit_price .value").html()*quantity/quan_base;
                       


	            $(this).parents(".son_wrap").find(".price .pricevalue").html(newtotal.toFixed(Num_After_dot)).formatCurrency({
	                groupDigits: true , symbol: '',
	                roundToDecimalPlace: temp_dropDecimals
	            });
                         
	          
	           
	            return false;
	        });

