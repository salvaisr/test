    function OpenDivPic(ImgName, ImgDivId) {
        var BigImgSrc = "<img  src='/images/itempics/" + ImgName + "_small50.jpg' alt='' />";
        if (lastImgName != ImgName || (document.getElementById("ViewIPPBigimg" + ImgDivId).style.display == 'none')) {
            document.getElementById("ViewIPPBigimg" + ImgDivId).innerHTML = BigImgSrc;
            document.getElementById("ViewIPPBigimg" + ImgDivId).style.display = '';
        }
        else
        {
            document.getElementById("ViewIPPBigimg" + ImgDivId).innerHTML = '';
            document.getElementById("ViewIPPBigimg" + ImgDivId).style.display = 'none';
        }
        lastImgName = ImgName
    }
