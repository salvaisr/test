                                $("body").on('click','.eventtracking',function()
                                      {
                                         var track_category = $(this).attr("track_category");
                                         var track_label    = $(this).attr("track_label");

                                         if ($(this).attr('track_action'))
                                            {
                                                var track_action = $(this).attr('track_action');
                                            }
                                            else
                                            {
                                                var track_action = "click";
                                            }


                                         if ($(this).hasClass("ee_promo"))
                                         {
                                             
                                                ga('ec:addPromo',
                                                 {
                                                  'id':  $(this).attr("ee_promo_id"),
                                                  'name': $(this).attr("ee_promo_name"),
                                                  'creative': $(this).attr("ee_promo_name"),
                                                  'position': $(this).attr("ee_promo_loc")
                                                 });
                                                 ga('ec:setAction', 'promo_click');
                                                 ga('send', 'event', 'Internal Promotions', track_action , 'click on promotion');
                                             
                                         }
                                         else
                                         {
                                             
                                                ga('send', 'event', track_category , track_action , track_label);
                                             
                                         }
                                      });

                                
                                    if ($(".ee_list").length)
                                    {
                                        
                                        $("body").on("click",".ee_product_click",function(evt)
                                            {
                                                
                                                var ee_list_obj         = $(this).parents(".ee_list");
                                                var ee_list_name        = $(ee_list_obj).attr("ee_list_name");
                                                var ee_list_category    = $(this).attr("ee_list_category");
                                                var ee_list_item_obj    = $(this).parents(".ee_list_item");
                                                var ee_list_counter     = $(ee_list_item_obj).attr("ee_list_item_position");
                                                var ee_this             = $(this);
                                                var ee_list_event_label = '';

                                                if (typeof $(ee_list_obj).attr("ee_list_event_label") !== typeof undefined && $(ee_list_obj).attr("ee_list_event_label") !== false)
                                                    {
                                                        ee_list_event_label = $(ee_list_obj).attr("ee_list_event_label");
                                                        if (ee_list_event_label=='product_name'){ee_list_event_label=$(ee_list_item_obj).find("[ee_list_itemname]").attr("ee_list_itemname");}
                                                    }
                                                if (ee_list_event_label == '') {ee_list_event_label = 'click on product';}

                                                var ee_list_event_action = '';
                                                if (typeof $(ee_list_obj).attr("ee_list_event_action") !== typeof undefined && $(ee_list_obj).attr("ee_list_event_action") !== false)
                                                    {
                                                        ee_list_event_action = $(ee_list_obj).attr("ee_list_event_action");
                                                    }
                                                if (ee_list_event_action == '') {ee_list_event_action = 'click';}

                                                var ee_list_event_category = '';
                                                if (typeof $(ee_list_obj).attr("ee_list_event_category") !== typeof undefined && $(ee_list_obj).attr("ee_list_event_category") !== false)
                                                    {
                                                        ee_list_event_category = $(ee_list_obj).attr("ee_list_event_category");
                                                    }
                                                if (ee_list_event_category == '') {ee_list_event_category = 'product';}

                                                ga('ec:addProduct',
                                                  {
                                                    'id':       $(ee_list_item_obj).find("[ee_list_itemid]").attr("ee_list_itemid"),
                                                    'name':     $(ee_list_item_obj).find("[ee_list_itemname]").attr("ee_list_itemname"),
                                                    'category': $(ee_list_item_obj).find("[ee_list_category]").attr("ee_list_category"),
                                                    'brand':    $(ee_list_item_obj).find("[ee_list_itembrand]").attr("ee_list_itembrand"),
                                                    'price':    $(ee_list_item_obj).find("[ee_list_itemprice]").attr("ee_list_itemprice"),
                                                    'position': ee_list_counter
                                                  });

                                                 ga('ec:setAction', 'click', {'list' : ee_list_name});
                                                 ga('send', 'event', ee_list_event_category, ee_list_event_action, ee_list_event_label,
                                                   {
                                                      'hitCallback': function()
                                                       {
                                                        //document.location = $(this).attr("href");
                                                       }
                                                   });

                                                   evt.stopPropagation();


                                                  // console.log('ee_list_event_category:' + ee_list_event_category);
                                                  // console.log('ee_list_event_action:' + ee_list_event_action);
                                                  // console.log('ee_list_event_label:' + ee_list_event_label);
                                                  // return false;
                                             });



                                            $(".ee_list").each(function()
                                            {
                                                var ee_list_name = $(this).attr("ee_list_name") ;
                                                //var ee_list_category = $(this).attr("ee_list_category_name") ;

                                                //console.log('list:' + ee_list_name);
                                                //console.log('category:' + ee_list_category);

                                                $(this).find(".ee_list_item").each(function()
                                                {

                                                    var ee_list_counter = $(this).attr("ee_list_item_position");
                                                   // console.log( $(this).find("[ee_list_itemid]").text() + '  = '  + $(this).find("[ee_list_itemname]").text() + ' = ' + ee_list_category + ' = ' +ee_list_counter);
                                                   if (ee_list_counter<16)
                                                   {
                                                    ga('ec:addImpression',
                                                    {
                                                      'id': $(this).find(".item-name a").attr("ee_list_itemid"),
                                                      'name':$(this).find(".item-name a").attr("ee_list_itemname"),
                                                      'category':$(this).find(".item-name a").attr("ee_list_category"),
                                                      'brand': $(this).find(".item-name a").attr("ee_list_itembrand"),
                                                      'list': ee_list_name,
                                                      'position': ee_list_counter
                                                    });
                                                    }
                                                      // console.log($(this).find("[ee_list_itemid]").text());

                                                });
                                            });
                                           // ga('send', 'pageview');
                                            //console.log('Send Analytics');

                                    }
                                      
                                   

                       
                    }
                 );

	    jQuery.fn.center = function ()
                      {
                            this.css("position","fixed");
                            var horizontalCenter = Math.floor(window.innerWidth/2 - ( $(this).outerWidth()/2));
                            var verticalCener = Math.floor(window.innerHeight/2 - ($(this).outerHeight()/2));
                            this.css("top", verticalCener + "px" );
                            this.css("left",horizontalCenter  + "px");
                            return this;
                        }

       
    

           function checkSectionfrm() {
                if (IsEmptyStr("document.memberFrm.UserName")) {
                    document.memberFrm.UserName.focus();
                    alert("לא הוזן שם משתמש".charRefToUnicode());
                    return false;
                }
                if (IsEmptyStr("document.memberFrm.IDNum")) {
                    document.memberFrm.IDNum.focus();
                    alert("סיסמא חייבת להכיל 4-20 תווים, יש להזין אותיות ו/או ספרות".charRefToUnicode());
                    return false;
                }
                return true;
            }

function flytocart(obj)
    {
                 
            var cart = $('#miniCart');
            var imgtodrag = $(obj).parents("li").find(".image img");
                        
            if (imgtodrag) {
                var imgclone = imgtodrag.clone()
                    .offset({
                        top: imgtodrag.offset().top,
                        left: imgtodrag.offset().left
                    })
                    .css({
                        'opacity': '0.5',
                        'position': 'absolute',
                        'height': '150px',
                        'width': '150px',
                        'z-index': '100'
                    })
                    .appendTo($('body'))
                    .animate({
                        'top': cart.offset().top + 10,
                        'left': cart.offset().left + 10,
                        'width': 75,
                        'height': 75
                    }, 1000, 'easeInOutExpo');
            
                setTimeout(function () {
                    cart.effect("shake", {
                        times: 2
                    }, 200);
                }, 1500);

                imgclone.animate({
                    'width': 0,
                    'height': 0
                }, function () {
                    $(this).detach()
                });
            }

            $('html, body').animate({
                'scrollTop' : $(cart).position().top
            }, 1000, 'easeInOutExpo');
 
    }

String.prototype.charRefToUnicode = function()
{
    return this.replace(
    /&#(([0-9]{1,7})|(x[0-9a-f]{1,6}));?/gi,
    function(match, p1, p2, p3, offset, s)
    {
        return String.fromCharCode(p2 || ("0" + p3));
    });
}




$( document ).ready(function()
{

       $(".navbar-toggler").click(function(){
            if ( $(this).parent().hasClass("active") )
            {
                $(this).parent().removeClass("active");
                 $(this).attr('aria-expanded', 'false');
            }
            else
            {
                $(this).parent().addClass("active");
                 $(this).attr('aria-expanded', 'true');
            }

       });


         $("body").on('mouseleave', '#floating-icons .icons-box',function (e) {
                            if (!$(e.target).is('input')) {
                              //   $('.content', this).removeClass("active");
                                $(this).removeClass("active");
                            }
                        });
  $("body").on('mouseenter', '#floating-icons .icons-box .icon', function () {
                           // $('.content', this).addClass("active");
                        $('#floating-icons .icons-box').removeClass("active");
                        $(this).parent().addClass("active");
                        
                        
                        if ( $(this).next().find("iframe").length  )
                            {
                                var iframedata = $(this).next().find("iframe").attr("data");
                                if ( iframedata != '' )
                                {
                                    $(this).next().find("iframe").attr("src" , iframedata);
                                    $(this).next().find("iframe").attr("data" , "");
                                }
                            }
                 })
                                     
                
                
username_validation()
 
 $("body").on("click","#usepointsbtn",function(){
            $.get("/services/CartServices.asp?service=points_cart_add", function(data)
            {
                if (data == "1")
                    {
                      RenderAllCarts();
                    }
            });
                if ( $(this).attr("href") == 'cart.asp' )
                {
                    location.href=$(this).attr("href");
                }
        return false;
    });
 
$("body").on("click","#points_delete_btn",function(){
            $.get("/services/CartServices.asp?service=points_cart_del", function(data)
            {
                if (data == "1")
                    {
                      RenderAllCarts();
                    }
            });
        return false;
    });

    $("body").on("click",".delete_coupon",function(){
            $.get("/services/CartServices.asp?service=deletecoupon", function(data)
