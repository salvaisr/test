                                            {
                                               $(this).click(function(){ return false;});
                                            });

                                            $("#item-relativeitems-popup").appendTo("body");
                                            $("#item-relativeitems-popup").show();
                                             $("#item-relativeitems-popup").addClass("removemainitem");
                                            $("#submitproductform").click(function(){document.addme.submit(); return false;});
                                        return false
                                     }
                             }
                            else
                            {
                                    return true;
                            }
                        return false;
                    });
                

          $("body").on("click","#Product_Checkout_Button",function()
              {
                if ($("#item-relativeitems-popup").length > 0 && typeof(bundleitems) == 'undefined')
                {
                    if ( $("#item-relativeitems-popup").is(":visible") )
                    {
                        document.addme.submit();
                    }
                    else
                    {
                        $("#item-relativeitems-popup .boxItem a").each(function()
                            {
                            $(this).click(function(){ return false;});
                            });

                        $("#item-relativeitems-popup").appendTo("body");
                        $("#item-relativeitems-popup").show();
                        $("#submitproductform").click(function(){document.addme.submit(); return false;});

                    }
                }
                else
                {
                    return true;
                }
                 return false;

              });


