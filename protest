                        $("body").on("change",".productbox_sons_select select",function()
                        {
                           $(this).parents("li").find(".product-box-prices .priceholder").html($('option:selected', this).attr('price'));
                                                                                                          
                             if ( $('option:selected', this).attr('regprice') != "0" )
                             {
