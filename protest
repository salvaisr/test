<script type="text/javascript">
    var $light_Slider = '';
        
    $(document).ready(function()
    {
         
        // add properties images to slider
        $(".PropertyImageHandle.image").each(function(){
            var properties_img_large = $(this).attr("data-large");
            if (properties_img_large !="" )
            {
                $('#imageGallery').append('<li data-thumb="'+$(this).attr("href")+'" data-src="'+properties_img_large+'" ><a href="#"><span></span><img src="'+$(this).attr("href")+'" /></a></li>');
            }
        });

        $(".properties_list_images option").each(function()
        {
            var productson_img_large = $(this).attr("image");
            if (productson_img_large != '' )
            {
                $('#imageGallery').append('<li data-thumb="'+productson_img_large+'" data-src="'+productson_img_large+'" ><a href="#"><span></span><img src="'+productson_img_large+'" /></a></li>');
            }
        });

        // add product sons images to slider
        $(".productlink.image").each(function()
        {
            if ( !$(this).hasClass("youtube") )
            { 
                var productson_img_large = $(this).attr("data_medium");
                $('#imageGallery').append('<li data-thumb="'+productson_img_large+'" data-src="'+productson_img_large+'" ><a href="#"><span></span><img src="'+productson_img_large+'" /></a></li>');
            }
        });
  
        $light_Slider = $('#imageGallery').lightSlider({
            gallery: true,
            item:1,
            loop:true,
            thumbItem:5,
            useCSS    : true, 
            cssEasing : 'ease',
            slideMargin:0,
            showThumbByDefault   : true,
            
            showAfterLoad : true,
            thumbnail:true,
            animateThumb : true, 
            thumbWidth : 100, 
            enableDrag: true,
            enableTouch: true,
            currentPagerPosition:'left',
            onSliderLoad: function(el) 
            {
                el.lightGallery({
                    showThumbByDefault: false,
                    selector: '#imageGallery .lslide',
                    youtubePlayerParams: {
                        modestbranding: 1,
                        showinfo: 0,
                        rel: 0,
                        controls: 1
                    },
                    vimeoPlayerParams: {
                        byline : 0,
                        portrait : 0,
                        color : 'A90707'  ,
                        autoplay: true ,
                        muted:true,
                        controls:false
                         
                    },
                });
                  
              var maxHeight = 0,
              container = $(el),
              children = container.children();
                children.each(function () {
                        var childHeight = $(this).height();
                        if (childHeight > maxHeight)
                        {
                            maxHeight = childHeight;
                        }
                });
                if (450 > maxHeight){
                    maxHeight = 450;
                }
                
                    container.height(maxHeight);
                
                //container.find("iframe").css("height", maxHeight);
                 $('#imageGallery').removeClass('cS-hidden');
            }   
        });  
    });    
</script>

