//	                 }catch (e){}

		

			    var CartParamsStr = '' ;
                $(".CartParams").each(function()
                   {

                      CartParamsStr = CartParamsStr + $(this).attr("id") + '{=}' +  $(this).val() + '{|}' ;
                   });

                   if (CartParamsStr != '' )
                   {
                       document.addme.CartParams.value = CartParamsStr;
                   }

                 
			

			    var relativepid                 = "";
                var relativequan                = "";
              


			    //add checked relative items
			        if ( jQuery(".relativecheckbox").length  > 0 )
	                   {

	                      $(".relativecheckbox").each(function()
	                      {
	                            if ($(this).is(":checked"))
	                            {
	                               relativepid = relativepid + ', '+ $(this).val();
	                               
	                               if ( $(this).parents(".boxItem").find("input[name='quantity']").length > 0 )
	                               {
	                                   relativequan = relativequan + ', ' + $(this).parents(".boxItem").find("input[name='quantity']").val();
	                               }
	                               else
	                               {
	                                   relativequan = relativequan + ', 1';
	                               }
	                              
                                   numOfItems = "1";
	                            }
	                      })

	                     if (relativepid != '')
	                        {
	                            document.addme.pid.value = document.addme.pid.value + relativepid;
	                            document.addme.quan.value = document.addme.quan.value + relativequan;
	                        }
                       }
                       
                        var product_package_params = "";
                        
 
