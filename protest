	  <script type="text/javascript">
    function OpenPaymentsWindow(obj) {
        $.get("/payments.asp?sum=" + total + "&ni=24&m=24", function(data)
        {
            lightpopup (data,'product_payments') ;
        });
        return false;
    }

	    function getCheckedValue(radioObj) {
            if(!radioObj)
                return "";
            var radioLength = radioObj.length;
            if(radioLength == undefined)
                if(radioObj.checked)
                    return radioObj.value;
                else
                    return "";
            for(var i = 0; i < radioLength; i++) {
                if(radioObj[i].checked) {
                    return radioObj[i].value;
                }
            }
            return "";
        }

        function calculate_package_items()
        {
           var pi_counter = 0;
           $(".packagelist .value").each(function(){
                pi_counter = pi_counter + parseInt ( $(this).val() );
           });
           $(".packagelist #product_counter").html(pi_counter);
        }


        function parseDate(str) {
            var mdy = str.split('/');
            var str = mdy[2] + '-' + mdy[1] + '-' + mdy[0];
            return new Date( str );
        }

        function calculate_date_range_quantity () {
            const oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds
            $(".property-date-range-quantity").each(function(){

               
                var property_id = $(this).find(".property-datepicker").eq(0).attr("propertyid");
                var property_selector = $(this).find(".property-datepicker").eq(0).attr("connectid");
                var property_value = $(this).find(".property-datepicker").eq(0).attr("property_value");

                $("#" + property_id ).val("");
 
                var firstDate  = $(this).find(".property-datepicker").eq(0).val() ;
                var secondDate = $(this).find(".property-datepicker").eq(1).val() ;

                if ( firstDate != "" && secondDate != "" ) {
 
                $("#" + property_selector ).val( '||' + property_id + '{=}{addtext}' + firstDate + ' - ' + secondDate );
  
                var firstDate  = parseDate( firstDate );
                var secondDate = parseDate( secondDate );
 
                const diffInMs   = new Date(secondDate) - new Date(firstDate)
                const diffInDays = diffInMs / (1000 * 60 * 60 * 24);
                $("#quant").val(diffInDays);
                $("#quant").trigger("change");
  
                }
            });
        }
	    $(document).ready(function()
	    {
            
            $(".property-date-range-quantity .property-datepicker").datepicker({
                        showOn: "button",
                        buttonImage: "/template_inc/eshopstoresframework/images/calendar_select.png",
                        buttonImageOnly: false,
                        onSelect: function(dateText)
                        {
                            calculate_date_range_quantity();
                        },
                        
                    });

