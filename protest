          {
                $("#its_btn").show();
                $(".product-prices.grid_6.omega .prices-box").show();
                $(".product-prices.grid_6.omega .order-button").show();
                $(".product_product_buttons .order-button").show();
                $(".deal-product .prices-box").show();
                $(".deal-product .order-button").show();
                $("#Product_AddToCart_message").removeClass("show");
                $("#Product_AddToCart_message").addClass("hide");
                return false;
          });
 
          
          $("body").on("click",".add-to-cart.dynamic",function()
            {
                        
                        var currentbutton = $(this);
                        
                            $(currentbutton).find("span").html("הוספה לסל");
                        
                        var tmp_quantity = $("#quant").val();

                        var attr = $(this).attr("quantity");
                        if (typeof attr !== typeof undefined && attr !== false)
                        {
                            tmp_quantity = attr;
                        }
               
                        url = addme_cart($(this).attr("itemid"),0,tmp_quantity,0,'','',1,'',1);
                       // console.log("url=" + url);
                        if (url!="")
                        {
                            
                            $(".product-prices.grid_6.omega .prices-box").hide();
                            $(".product-prices .order-button").hide();
                            $(".product_product_buttons .order-button").hide();

                            $("#Product_AddToCart_message").removeClass("hide");
                            $("#Product_AddToCart_message").addClass("show");
                            $("#Product_AddToCart_message").html("<div class='alert alert-success' style='direction:ltr'>...אנא המתן</div>");
                             
 

                            $.post( param_cartlogic_url, { Ajax: param_Ajax, pid: param_pid, pip_str: param_pip_str, pip_prices: param_pip_prices, pip_ids: param_pip_ids, quan: param_quan, RequestGiftInfo: param_RequestGiftInfo, m: param_m, BuildComputerItems: param_BuildComputerItems  }).done(function( data ) {
                               //console.log(data);
                               if (data=="0")
                               {
                                    $("#Product_AddToCart_message").html("<div class='alert alert-danger'><a href='#' class='closeit'>X סגירה</a> לא ניתן לרכוש את הכמות המבוקשת ממוצר זה</div>");
                               }
                               else
                               { 
                                    
                                        $(currentbutton).find("span").html("הפריט נוסף לסל");
                                        $(".product-prices.grid_6.omega .prices-box").show();
                                        $(".product-prices .order-button").show();
                                        $(".product_product_buttons .order-button").show();
                                        $("#Product_AddToCart_message").removeClass("show");
                                        $("#Product_AddToCart_message").addClass("hide");
                                    
                                    try
                                    {
	                                    quan = $("#miniCart #cartQuantity").html();
                                        $("#miniCart #cartQuantity").html((quan*1) + 1);
                                    }catch(err){}
                                     
                                    OpenSmallCart();
                                   
                                }
                                RenderAllCarts();
                             })
                        }
                          
                         return false;
                  }
          );

