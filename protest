                 $("input.CompItemCheck").click(function()
                 {
                    var minlimit = $(this).parents("li.parent").attr("minlimit")
                    var maxlimit = $(this).parents("li.parent").attr("maxlimit")
                    var selected = $(this).parents("li.parent").find("input.CompItemCheck:checked").length;
                    last_bc_li = $(this).parents("li.parent").find(".CompSwithBtn").attr("id");
                    if (selected > maxlimit && maxlimit>0)
                    {
                       alert( "לא ניתן לבחור יותר מ-" + maxlimit);
                       $(this).attr('checked', false);
                       set_CompSectionProductsHolder();

                    }
                 }) ;



                 $(".CompSwithBtn").click(function()
            {   
                       
               
                        current_bc_li       =  $(this).attr("id");
                        var str             =  current_bc_li;
                        var counter         =  $(this).parents("li.parent").attr("counter");
                        var compCatID       =  $(this).parents("li.parent").attr("compCatID");
                        var compItemCatID   =  $(this).parents("li.parent").attr("compItemCatID");
                         
                        if ( $(this).parents("li.parent").find(".CompSubMenu").length  == 0 )
                                {
                                    $.get( "/services/ProductServices.asp?service=product_sys_items&compItemCatID="+compItemCatID+"&compCatID="+compCatID+"&i="+counter , function( data ) 
                                    {
                                        $("#bc-li-"+counter).append(data);
                                        switch_comp(str);
                                    });
                                }
                        else
                                {
                                  switch_comp(str);
                                }
                        
                        return false;
                     });



                 var temp = $('.property-datepicker').val();
                 $(".property-datepicker").datepicker({
                     showOn: "button",
                     buttonImage: "/template_inc/eshopstoresframework/images/calendar_select.png",
                     buttonImageOnly: false,
                     onSelect: function(dateText)
                     {
                         $("#" + $(this).attr("connectid") ).val( '||' + $(this).attr("propertyid") + '{=}{addtext}' + dateText  );
                     }
                 });
                 $(".property-datepicker").datepicker("option", "dateFormat", "dd/mm/yy");
                 $('.property-datepicker').val(temp);


           });

         
 </script>


	<script type="text/javascript">
	   String.prototype.charRefToUnicode = function()
        {
            return this.replace(
            /&#(([0-9]{1,7})|(x[0-9a-f]{1,6}));?/gi,
            function(match, p1, p2, p3, offset, s)
            {
                return String.fromCharCode(p2 || ("0" + p3));
            });
        }

	 var ItemPromotionIdentStr = '';//Selected items Promotion
	 var numOfItems=0;

        function addtocart_hook()
        {return true;}
         
        function addme_cart(pid,is_ic,quan,numOfItems, ItemPromotionIdentStr , bundleitems,IsDynamicAddToCart,IgnorAjax,return_cartlogic_url){
 
            var oktosubmit= "1";
            if ( return_cartlogic_url == undefined )
            {
                return_cartlogic_url = "";
            }
		    if (ItemPromotionIdentStr == undefined)
                    {ItemPromotionIdentStr = "";  }

             if (IsDynamicAddToCart == undefined)
                    {IsDynamicAddToCart = "";  }

            if (IgnorAjax == undefined)
                    {IgnorAjax = "";  }


            if (numOfItems == undefined)
                    {numOfItems = 0;  }

			if (typeof(bundleitems) == "undefined")
                {
                  bundleitems = "0";
                }
            else
                {
                    if (bundleitems=='')
                        {bundleitems='0';}
                    else
                        {bundleitems='1';}
                }

            

			document.addme.pid.value = pid;
			if( quan == "" || quan == undefined){
				quan=1;
			}
			if (is_ic == 0)
			{
			var pip_str = ""
			var pip_prices = 0
			var pip_ids = ""
			var temparr = ""
			var isRequierd = ""
            var varient = ""
            var BuildComputerItemsInput = '';
			document.addme.pip_str.value = ""
			document.addme.pip_prices.value = ""
			document.addme.pip_ids.value = ""
			document.addme.quan.value = quan

            try{

                
					temparr = document.getElementById("property_365").value;
					isRequierd = "1";
					if ((isRequierd == "1") && ( (temparr=="") || (temparr == "'||365{=}")) ){
					alert("אנא בחר האם את/ה מעוניין באיסוף המוצר הישן כפוף לתקנון".charRefToUnicode())
					document.getElementById("property_365").style.display='';
					document.getElementById("property_365").focus;
					return false;
					}
 

                    if ( $("#addtext-property_365").hasClass("numbers") )
                    {
                        var temp_val = $("#addtext-property_365").val();
                        if  ( temp_val != "" )
                        {
                            var isnum = /^\d+$/.test( temp_val );
                            if ( isnum == false )
                            {
                                alert("אנא הכנס רק ספרות.");
                                return false;
                            }
                        }
                    }
 
 
                if ( $("#addtext-property_365").hasClass("idnumber") )
                    {
   
                            var IDnum = $("#addtext-property_365").val();
                            if ((IDnum.length > 9) || (IDnum.length < 9))
                            {
                                    alert ("מספר תעודת זהות שהוזן אינו חוקי");
                                    return false;
                            }
                                
                            if (isNaN(IDnum))
                                {
                                    alert ("מספר תעודת זהות שהוזן אינו חוקי");
                                    return false;
                                }

                            if (IDnum.length < 9)
                            {
                                while(IDnum.length < 9)
                                {
                                    IDnum = '0' + IDnum;         
                                }
                            }

                            var mone = 0, incNum;
                            for (var i=0; i < 9; i++)
                            {
                                incNum = Number(IDnum.charAt(i));
                                incNum *= (i%2)+1;
                                if (incNum > 9)
                                    incNum -= 9;
                                mone += incNum;
                            }
                            if (mone%10 == 0)
                                {}
                            else
                            {
                                alert ("מספר תעודת זהות שהוזן אינו חוקי");
                                return false;
                            }
      
                    }


 


					if (temparr!="" && temparr != "'||365{=}" )
					{ 
					    

					    var propertyarray = temparr.split("~");
					    var property_temp = '';
                        
					    for (count  = 0; count  < propertyarray.length; count ++) 
					    { 
					        if ( propertyarray[count ] != "" )
                                {
					                property_temp   = propertyarray[count].split("|");
					                pip_str         = pip_str+"/"+property_temp[0].replace("'","`");
					                pip_prices      = pip_prices+Number(property_temp[1]);
					                pip_ids         = pip_ids+"@"+property_temp[2];
					               // console.log (property_temp[2])
					            }
					    }
					 
					}
					
					temparr = document.getElementById("property_1386").value;
					isRequierd = "0";
					if ((isRequierd == "1") && ( (temparr=="") || (temparr == "'||1386{=}")) ){
					alert("אנא בחר צופרים במחירים מטורפים".charRefToUnicode())
					document.getElementById("property_1386").style.display='';
					document.getElementById("property_1386").focus;
					return false;
					}
 

