                if ( $("#addtext-property_3066").hasClass("idnumber") )
                    {
   
                            var IDnum = $("#addtext-property_3066").val();
                            if ((IDnum.length > 9) || (IDnum.length < 9))
                            {
                                    alert ("מספר תעודת זהות שהוזן אינו חוקי");
