                            { }
                    
                       
                      cartlogic_url = "/CartLogic.asp?Ajax=1&pid=" + pid+bundlepid+relativepid + "&pip_str=" + pip_str + "&pip_prices=" + pip_prices + "&pip_ids=" + pip_ids + "&quan=" + quan+bundlequan+relativequan + "&RequestGiftInfo=" + ItemPromotionIdentStr + "&m=" + numOfItems + "&BuildComputerItems=" + BuildComputerItemsInput + "&CartParams=" + CartParamsStr+product_package_params;
                      param_cartlogic_url = "/CartLogic.asp?CartParams=" + CartParamsStr+product_package_params;
                      param_Ajax                  = "1";
