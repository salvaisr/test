	    function PropertyImageHandle(PropertyId,ImageId,PropertValue)
            {
                UpdSelectedTxt('SelectedTxt'+PropertyId,PropertValue,'property_'+PropertyId);
                document.getElementById('property_'+PropertyId).value = PropertValue;

                return false;
            }

         function PropertyTextHandle(PropertyId,obj)
            {
                 var string = $(obj).val();
                 //string = string.replace("p", "&#112;");
                 //string = string.replace("P", "&#80;");

                 

                 $('#property_'+PropertyId).val('||'+PropertyId+'{=}{addtext}'+ string );
                 return false;
            }

         function PropertyCheckboxHandle(PropertyId,ImageId,PropertValue,obj)
         {   
             var properties = '';

             

                 $(obj).parents(".property-checkbox").find(".properties").val("");
                $(obj).parents(".property-checkbox").find("input[type=checkbox]").each(function(){
                     if  ( $(this).is(":checked") )
                     {
                         properties = properties + $(this).attr("property_value") + "~";
                     }
                 });
                $(obj).parents(".property-checkbox").find(".properties").val(properties);  

             
             return false;
         }

       function PropertyLinkHandle(PropertyId,ImageId,PropertValue,obj)
            {
                $(obj).parents(".ProperyLinkDiv").find("a").removeClass('current');
                $(obj).parents(".ProperyLinkDiv").find("a").removeClass('active');
                $(obj).parent().parent().find("p").removeClass('selected');
                $(obj).addClass("current");
                $(obj).addClass("active");
                $(obj).parents(".ProperyLinkDiv").addClass('selected');
                document.getElementById('property_'+PropertyId).value = PropertValue;
                return false;
            }

       function change_total_hook()
       {}

	   function change_total(valu)
       {

	       var quan_base = $(".ProductPageSection").attr("quan_base");
	       if ( quan_base == "" )
	       {
	           quan_base = 1;
	       }
	       var tax = 1.17;

	       var quantity = quan_base;
			if($("#quant").length > 0) {
					quantity =  $("#quant").val();
			}
			if(!$.isNumeric(quantity))
			{ 
			    quantity = quan_base;
			    $("#quant").val(quantity);
			}
			if(quantity=="0")
			{
			    quantity = quan_base;
			    $("#quant").val(quantity);
			}
			
			var quantity_clc = quantity / quan_base;
            
			start_regularprice  = 3690*quantity_clc*1;
			start_price         = 3150*quantity_clc*1;
			start_price         = start_price*1+0*1;
			total               = start_price;
			total1              = total;


           if ( $("select#product-sons-select").length  > 0  )
            {
               if ($("select#product-sons-select") != "")
               {
                   start_price = $("select#product-sons-select option:selected").attr("price");
                   total = start_price * quantity;
                   total1 = total;
               }
               if (typeof (start_price) == "undefined")
               {
                    start_price = $("#total").attr("price");
                    total = start_price * quantity;
                    total1 = total;
               }
           }
          
           
			if ( valu == "bundle" )
			{  
			    if  ( $("#bundle-sons-select").length  > 0 )
			    {
			        if  ( $(".bundlecheckbox.main").length == 0 )
			        {
			           
			            start_price =  $('#bundle-sons-select option:selected').attr('price');
			             
			        }
			        else
			        {
			            start_price =  $(".bundlecheckbox.main").attr("price");
			        }


			         
			        start_price         = start_price*1+0*1;
			        total               = start_price;
			        total1              = total;
			    }
			    
			
			}
			 

            //console.log('1)' + start_regularprice);

             
             

            if ( $("#productsonslinksbox .productlink.current").length > 0  )
            {
                start_price         = $("#productsonslinksbox .productlink.current").attr("price");
                total               = start_price*quantity_clc;
                total1              = total;
                start_regularprice  = $("#productsonslinksbox .productlink.current").attr("reg_price");
                if (start_regularprice == "")
                {start_regularprice=total;}
                else
                {start_regularprice=start_regularprice*quantity_clc;}

            }

            if ( $("#productsonsimgbox .productlink.current").length > 0  )
            {
                start_price         = $("#productsonsimgbox .productlink.current").attr("price");
                total               = start_price*quantity_clc;
                total1              = total;
                start_regularprice  = $("#productsonsimgbox .productlink.current").attr("reg_price");
                if (start_regularprice == "")
                {start_regularprice=total;}
                else
                {start_regularprice=start_regularprice*quantity_clc;}
               
            }

          // console.log('1)' + start_regularprice);

			
                        Propertystr = $("#property_365").val() ;
                        var propertyarray = Propertystr.split("~");
					   
                        for (count  = 0; count  < propertyarray.length; count ++) 
                         { 
					        if ( propertyarray[count ] != "" )
                                {
					                arrProperty   = propertyarray[count].split("|");
					                if (isNaN(arrProperty[1]) == false)
                                    {
                                        total = total + (Number(arrProperty[1])*quantity_clc)
                                        total1=total.toFixed(2)
                                        start_regularprice = start_regularprice + (Number(arrProperty[1])*quantity_clc)
                                    }
                                }
                          }
            
            
                        Propertystr = $("#property_1386").val() ;
                        var propertyarray = Propertystr.split("~");
					   
                        for (count  = 0; count  < propertyarray.length; count ++) 
                         { 
					        if ( propertyarray[count ] != "" )
                                {
					                arrProperty   = propertyarray[count].split("|");
					                if (isNaN(arrProperty[1]) == false)
                                    {
                                        total = total + (Number(arrProperty[1])*quantity_clc)
                                        total1=total.toFixed(2)
                                        start_regularprice = start_regularprice + (Number(arrProperty[1])*quantity_clc)
                                    }
                                }
                          }
            
            
                        Propertystr = $("#property_3066").val() ;
                        var propertyarray = Propertystr.split("~");
					   
                        for (count  = 0; count  < propertyarray.length; count ++) 
                         { 
					        if ( propertyarray[count ] != "" )
                                {
					                arrProperty   = propertyarray[count].split("|");
					                if (isNaN(arrProperty[1]) == false)
                                    {
                                        total = total + (Number(arrProperty[1])*quantity_clc)
                                        total1=total.toFixed(2)
                                        start_regularprice = start_regularprice + (Number(arrProperty[1])*quantity_clc)
                                    }
                                }
                          }
            
            

        
            var relativeprice = 0 ;
               $(".item-relativeitems .relativecheckbox:checked").each(function()
               {
                    relativeprice =  $(this).attr("price");
                    total  = total + (Number(relativeprice)*1)
                    total1 = total.toFixed(2)
                     start_regularprice = start_regularprice + (Number(relativeprice)*1)
               });

		   	
              var rl_total = 0;
              var rl_regprice_total =0;
              var rl_isrebate= 0;
              $("#bundlesale .bundlecheckbox:checked").each(function()
	                {
	                    var rl_price    = $(this).attr("price");
	                    var rl_parent   = $(this).attr("parent");
	                    var rl_regprice = $(this).attr("regprice");

                        if (rl_parent != "0" )
	                    {
	                        rl_total          = (rl_total*1) + (rl_price.replace(",","")*1);
                            rl_regprice_total = (rl_regprice_total*1) + (rl_regprice.replace(",","")*1);
                            if ($(this).attr("rebateprice")>0)
                            {
                                rl_isrebate=1;
                            }
	                    }

	                });


		      if(document.all){
                     rightProperty="innerText";
                }
                else{
                    rightProperty="textContent";
                }

                try {
                      //eval("document.getElementById('total')."+rightProperty+" = "+total.toFixed(2));
                      var Num_After_dot = "2";
                      var temp_dropDecimals = 0;
                      if (Num_After_dot == "1" || Num_After_dot == "2" )
                      {
                        temp_dropDecimals = 2;
                      }
                      
                     /* $("#total").html(total.toFixed(Num_After_dot)).formatCurrency({
                          groupDigits: true , symbol: '',digitGroupSymbol: ',',
                          roundToDecimalPlace: temp_dropDecimals
                        });*/
                     
                        $(".BuildComputerfooter .price span").html(total.toFixed(Num_After_dot)).formatCurrency(
                        {
                            groupDigits: true , symbol: '',digitGroupSymbol: ',',
                         roundToDecimalPlace: temp_dropDecimals
                        });
                        
                        
                           
                         var bundleprice = (total*1) + (rl_total*1);
                         var rl_total_save = 0
                         rl_total_save = (rl_regprice_total)+(total*1)-bundleprice

                         var savetext = $(rl_regprice_total.toFixed(Num_After_dot)).formatCurrency({
                         groupDigits: true , symbol: '',digitGroupSymbol: ',',
                         roundToDecimalPlace: temp_dropDecimals

                        });

                         $(".bundlesavepricetext").hide();
                         $("#bundlesaveprice").html("");
                        if (rl_total_save>0 && rl_isrebate==1)
                        {
                            $(".bundlesavepricetext").show();
                              $("#bundlesaveprice").html(rl_total_save.toFixed(Num_After_dot)).formatCurrency({
                                  groupDigits: true , symbol: '',digitGroupSymbol: ',',
                             roundToDecimalPlace: temp_dropDecimals
                            });
                        }
                        


                        $("#bundleprice").html(bundleprice.toFixed(Num_After_dot)).formatCurrency({
                            groupDigits: true , symbol: '',digitGroupSymbol: ',',
                         roundToDecimalPlace: temp_dropDecimals

                        });

                        
                            $("#total_regularprice").html(start_regularprice.toFixed(Num_After_dot)).formatCurrency({
                                groupDigits: true , symbol: '',digitGroupSymbol: ',',
                                roundToDecimalPlace: temp_dropDecimals

                            });

                            $(".toal_regularprice_view").html(start_regularprice.toFixed(Num_After_dot)).formatCurrency({
                                groupDigits: true , digitGroupSymbol: ',' , symbol: '', 
                                roundToDecimalPlace: temp_dropDecimals

                            });
                    
                            $(".toal_saleprice_view").html(total.toFixed(Num_After_dot)).formatCurrency({
                                groupDigits: true , symbol: '',digitGroupSymbol: ',',
                                roundToDecimalPlace: temp_dropDecimals
                            });
                        

                    }
                catch(err)
                        {}
               
                try
                 {

                     var NewPayment =  (((PaymentRibit/100*total)+(total*1))/payments) ;
                       try
                       {

                         document.getElementById("Newpayments").innerHTML = NewPayment.toFixed(Num_After_dot);
                       }
                        catch(err)
                        {}

                       
                        var currentprice = NewPayment * payments;
                     // document.getElementById("total2").innerHTML  =  currentprice.toFixed(Num_After_dot) + ' <em>₪</em>';
                         $(".properties-price #total2").html(currentprice.toFixed(Num_After_dot) + ' <em>₪</em>');
                     


                 }
                 catch(err)
                 {}

                 try
                 {
                    if ( $(".monthly-payments-list").length )
                    {
                        $(".monthly-payments-list .monthly-payments").each(function(){
                            var list_payment = $(this).find(".payments").html();

                            if ( $(this).find(".value").hasClass("withribit"))
                             {

                             }
                             else
                             {
                                 $(this).find(".value").html( (total1/list_payment).toFixed(Num_After_dot) );
                             }
                            
                        });
                    }
                         
                     var temp_payment = $(".product-details-ul .monthly-payments .payments").html();

                     if ($(".product-details-ul .monthly-payments .price .value").hasClass("withribit"))
                     {

                     }
                     else
                     {
                        $(".product-details-ul .monthly-payments .price .value").html( (total1/temp_payment).toFixed(Num_After_dot) );
                     }
                 
                    document.builditemsprint.PropertiesStr.value = PropertiesStr;
                    document.getElementById("AdditionalPrice").innerHTML =  total1 - start_price;
                    var CurrentPayNum = document.getElementById("maxPaymentSpan").innerHTML;
                    CalcPaymentInt(CurrentPayNum);
                 }
                 catch(err)
                 {}
                  
                 change_total_hook(valu);
              }

		function  ShowImages(objstr)
            {

                var objarray = objstr.split("|")
                if (objarray[5] == "1" )
                {
                var PropertyId = objarray[4];
                var ImageId = objarray[3];

                var ImgSrc = "<img src='/images/itempics/Proprties/Property_"+PropertyId+"_"+ImageId+"_m.jpg'  alt=''/>";
                document.getElementById("ViewIPPimg"+PropertyId).innerHTML = ImgSrc;
                }
            }


		 
		 



		function fw_SwitchSelect(SelectName,SelectId,num)
          {
            CurrentSelect = document.getElementById(SelectName)

            if (CurrentSelect.style.display == 'none')
                {
                 document.getElementById(SelectId).style.display = '' ;
                 CurrentSelect.style.display = '';
                     if (typeof num!= 'undefined')
                       {
	                        SelectedTbl = $("#" + "dataTbl"+num );
	                        //SelectedTbl.style.display = 'none';
                            $(SelectedTbl).hide();
                        }
                }
            else
                {
                CurrentSelect.style.display = 'none';
                document.getElementById(SelectId).style.display = 'none' ;
                }
          }

          function UpdSelectedTxt(SpanTxtId,InsertTxt,DivId,num)
          {
            if ( InsertTxt !="" )
                {
                    var TxtStr      = InsertTxt.split("|")
                    var ItemName    = TxtStr[0].split(":")
                    $("#" + SpanTxtId).html("" + ItemName[1]);
                    //document.getElementById(SpanTxtId).innerHTML = "" + ItemName[1]
                    if (typeof num!= 'undefined')
                        {
                            //SelectedTbl = document.getElementById("dataTbl"+num);
                            //SelectedTbl.style.display = 'block';
                            SelectedTbl = $("#" + "dataTbl"+num );
                                //SelectedTbl.style.display = 'none';
                                $(SelectedTbl).show();
                        }
                }
            else
                {  
                    $("#" + SpanTxtId).html("");
                    //document.getElementById(SpanTxtId).innerHTML = "";}
                }
             

            try{ShowBuildComputerImages();}
            catch(err){}
            change_total();
            try {UpdateForumsData();}
            catch(err){}

           }
           
           
            
                  function reviews_show()
                      {
                        var review_hash = window.location.hash.substr(1);
                        if ( review_hash == "product_rate" )
                                {
                                    if ( $(".product-tabs-horizontal #product_rate").length > 0 )
                                        {
                                            var review_tab_index = $(".product-tabs-horizontal #product_rate").parents(".tabs-block").index();
                                            review_tab_index = review_tab_index - 1;
                                            $( ".ui-tabs-nav li:eq("+review_tab_index+") a " ).trigger("click");
                                        }
                                }
                      }
            

           $(document).ready(function()
           {

                //reviews in tabs
                $("body").on("click", ".product_small_rate_div a", function () {
                   
                    if ( $(".product_product_review").parent().hasClass("es_accordion") )
                    {
                        $(".product_product_review").prev().click();
                        $('html, body').animate({
                            scrollTop:  $(".es_accordion").offset().top
                        }, 500);
                    }
                     
                    if ( $(".product_product_review").parent().hasClass("es_accordion") )
                    {   
                        var tabcounter = 0;
	                    $(".product_product_review").parent().find(".designbox").each(function(){
	                        if ( $(this).hasClass("product_product_review") )
                            {
                             			
                                    $(this).parent().find(".nav-link:eq("+tabcounter+")").click();
                            }
                            tabcounter = tabcounter + 1;
                        });
                    }

                    if ( $(".product_product_review").parent().hasClass("tabs") )
                    {     
                        var tabcounter = 0;
                        $(".product_product_review").parent().find(".designbox").each(function () {
                            if ( $(this).hasClass("product_product_review") )
                            {
                                 $(this).parent().find(".nav li:eq("+tabcounter+")").find(".nav-link").click();
                            }
                            tabcounter = tabcounter + 1;
                        });
                    }

                    if ( $("#header").find(".fixed-top").length )
                    {
                         $('html, body').animate({
                            scrollTop: $(".product_product_review").offset().top - $("#header").find(".fixed-top").height()
                        }, 500);
                         
                    }

                    if ($("body").hasClass("responsive"))
                    {  
                         $('html, body').animate({
                            scrollTop: $(".product_product_review").offset().top - $("#mobile_header").height()
                         }, 500);
                    }
                });

               


                $(".properties-package  .select").on("click",function(){

                   if ( $(this).parents(".properties-group").find(".properties-group-item_check:checked").length  == 0 )
                    {
                        alert("בחר מאפיין");
                        return false;
                    }

                      var selected_radio  = $(this).parents(".properties-group").find(".properties-group-item_check:checked");
                      var selected_parentid = $(this).parents(".properties-group").find(".properties-group-item_check:checked").attr("parentid") ;
                      var selected_text = $(this).parents(".properties-group").find(".properties-group-item_check:checked").parent().find("span.label").text();


                        $(".properties-group-item_check").each(function()
                           {
                               if($(this).is(":checked"))
                               {
		                           var property_parentid = $(this).attr("parentid") ;
                                   if ( property_parentid != '' && property_parentid !=  '0' )
                                       {
                                           $(".properties-group-item_check").each(function()
                                               {
                                                  if ( $(this).attr("parentid") == selected_parentid )
                                                   {
                                                        //console.log (property_parentid);
                                                        $(this).attr('checked', false);
                                                        $(this).parents(".properties-package").find(".property_selected").val("");

                                                   }
                                               });
                                       }

                               }

                           });

                     $(selected_radio).attr('checked', true);
                     $(selected_radio).parents(".properties-package").find(".property_selected").val(  $(selected_radio).val() );
                     $(selected_radio).parents(".properties-package").prevAll(".properyparentname:first").find(".selected_property_name").html(  ' : ' + selected_text )  ;
                     $(this).parents(".properties-package").removeClass("active");
                     $(this).parents(".properties-package").addClass("selected");
                     $(".ProductPageSection .properties-group").hide();
                    change_total();
                    return false;
                });

                $(".properties-package  .cancel").on("click",function(){

                    $(this).parents(".properties-group").find(".properties-group-item_check:checked").prop('checked', false);

                    $(this).parents(".properties-package").removeClass("active");
                    $(this).parents(".properties-package").removeClass("selected");
                    $(".ProductPageSection .properties-group").hide();
                    $(".property_selected").each(function(){
                        if ( $(this).val() != "" )
                            {
                                $(this).parents(".properties-package").addClass("selected");
                            }
                    });
                    change_total();
                    return false;
                });

                $(".properties-package .ProperyName").click(function(){
                            

                 $(".properties-group-item_check").each(function()
                        {
                          if ( $(this).parents("li").hasClass("selected") == false )
                             {
                                $(this).parents("li").removeClass("active");
                                $(this).parents("li").removeClass("selected");
                            }
                        });
                         
                            var property_parentid = $(this).parents("li").find(".properties-group-item_check").attr("parentid") ;
                            
                            if ( property_parentid != '' && property_parentid !=  '0' )
                            {
                                $(".properties-group-item_check").each(function()
                                {
                                    if ( $(this).attr("parentid") == property_parentid )
                                    {
                                        $(this).parents("li").removeClass("active");
                                        $(this).parents("li").removeClass("selected");
                                    }
                                });
                            }


                        if ( $(this).parents(".properties-package").hasClass("active") )
                            {
                                //$(this).parents(".properties-package").removeClass("active");
                            }
                            else
                            {
                                $(this).parents(".properties-package").addClass("active");
                                $(this).parents(".properties-package").find(".properties-group").show();
                            }
                      });


                    $(".properties-group-item img").on('mouseenter', function()
                    {
                       var prg_path   = $(this).attr("bigimage");
                       var prg_parent = $(this).parent("div") ;

                        if ( $(prg_parent).find(".bigimage").length == 0 )
                        {
                            var html = "<img class='bigimage' src='"+ prg_path +"'>" ;
                            $(html).appendTo(prg_parent);

            }
                    $(prg_parent).find(".bigimage").show();
            }).on('mouseleave', function(){
                          var prg_parent = $(this).parent("div") ;
                        $(prg_parent).find(".bigimage").hide();
                    });


                
                        reviews_show();
                        $(".product_small_rate_div a").click(function(){
                            if ( $(".product-tabs-horizontal #product_rate").length > 0 )
                            {
                                var review_tab_index = $(".product-tabs-horizontal #product_rate").parents(".tabs-block").index();
                                review_tab_index = review_tab_index - 1 ;
                                $( ".ui-tabs-nav li:eq("+review_tab_index+") a " ).trigger("click");
                            }
                        });
                

					$("#quant").change(function(){
						change_total();
					});
                 
					$(".quantplus").click(function(){
						var quantity      = $("#quant").val();
                        var quan_multiply = $(".ProductPageSection").attr("quan_multiply");
                        var quantity_calc = quantity * 1 + (quan_multiply * 1);
                        
                        if ( quantity == 1 && quan_multiply > 1 ) {
                            quantity_calc = quantity * 1 + (quan_multiply * 1) - 1;
                        }



                        
 
                        $("#quant").val( quantity_calc );
						change_total();
						});

					$(".quantminus").click(function(){
						var quantity = $("#quant").val();
                        var quan_multiply = $(".ProductPageSection").attr("quan_multiply");
						if( quantity > 0 )
                        {
                            var quantity_calc = quantity - quan_multiply * 1;
                            
    
						    $("#quant").val( quantity_calc );
						    change_total();
						}
					});

                    
       

                        $('#product-deliverydate').focus(function(){
                            $('#datepicker').datepicker('show');
                        });


                    var temp = $('#product-deliverydate').val();
                    var dateToday = new Date();
                    var hour = dateToday.getHours();
                    var minDate = dateToday;
                   if( dateToday.getDay() == 5 && hour > 14 )
                        {
                        minDate = "+2";
                        }

                        
                    $("#product-deliverydate").datepicker({
                        showOn: "button",
                        buttonImage: "/template_inc/eshopstoresframework/images/calendar_select.png",
                        buttonImageOnly: false,
                        minDate:  minDate,
                        onSelect: function(dateText)
                        {
                            //$("#" + $(this).attr("connectid") ).val( '||' + $(this).attr("propertyid") + 'p{addtext}' + dateText  );
                        },
                           beforeShowDay: function(date) {
                           var show = true;
                           if( date.getDay()==6 ) { show=false }
                           return [show];
                        }
                    });
                        
                    
                        
                    $("#product-deliverydate").datepicker("option", "dateFormat", "dd/mm/yy");
                    $('#product-deliverydate').val(temp);
                    $("#todaydate").click
                    (function() {
                        var today = new Date();
                        today.setDate(today.getDate());
                        cmonth = today.getMonth() + 1;
                        var todaydate = (today.getDate()) + '/' + cmonth + '/' + today.getFullYear();
                        $("#product-deliverydate").val(todaydate);
                        return false;
                    });

                    $("#tommorowdate").click
                    (function() {
                        var today = new Date();
                        today.setDate(today.getDate() + 1);
                        cmonth = today.getMonth() + 1;
                        var todaydate = (today.getDate()) + '/' + cmonth + '/' + today.getFullYear();
                        $("#product-deliverydate").val(todaydate);
                        return false;
                    });


