                          $("#reviewform").validate({
                                    submitHandler: function(form)
                                        {

                                            var theform    = $(form).serialize();
                                            var url     = $(form).attr("action");

                                             $.post(url, theform, function (data)
                                              {
                                                 
                                                   
                                                 if (data == "1" )
                                                 {
                                                   $(form).replaceWith("<div class='success'>חוות הדעת נשלחה בהצלחה!</div>");
                                                 }
                                                 if (data == "0")
                                                 {
                                                      alert("נא פתור נכון את שאלת האבטחה ונסה שנית");
                                                 }
                                                  if (data == "")
                                                  {
                                                    alert("חלה תקלה בשליחת התגובה אנא נסה שנית");
                                                  }
                                              })
                                            return false;
